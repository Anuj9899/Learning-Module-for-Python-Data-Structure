#Creation of Node
class Node:
    def __init__(self,data):
        self.data=data
        self.link=None
#Linked List Craetion
class ll:
    def __init__(self):
        self.head=None
#Print(Traversing of linked list)
    def print(self):
        if self.head==None:
            print("linked list is empty")
        else:
            n=self.head
            while n!=None:
                print(n.data,end="-->")
                n=n.link
#Insertion at the begining
    def insertion_at_beg(self,data):
        newnode=Node(data)
        newnode.link=self.head
        self.head=newnode





#Object reference
t=ll()
#insertion of Linked list
t.insertion_at_beg(34)
t.insertion_at_beg(35)
t.insertion_at_beg(36)
t.insertion_at_beg(37)
#print the linked list
t.print()

